{
  "name": "github-search",
  "version": "1.0.0",
  "description": "![github search](./images/Profile-screen.png)",
  "main": "app.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "heroku-postbuild": "webpack -p --config webpack.config.js --progress && webpack -p --config webpack.config.server.js --progress",
    "buildProd": "rm -rf dist && webpack --mode development webpack.config.js && webpack --mode production --config webpack.config.server.js",
    "buildDev": "rm -rf dist && webpack --mode development webpack.config.js && webpack --mode development --config webpack.config.server.js",
    "dev": "rm -rf dist && webpack --mode development webpack.config.js && webpack --mode development --config webpack.config.server.js && webpack-dev-server --mode development",
    "transpile": "babel ./bin/server.js --out-dir dist-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabogarciam/github-search.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabogarciam/github-search/issues"
  },
  "homepage": "https://github.com/gabogarciam/github-search#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
